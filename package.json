    "api:convert": "swagger2openapi ../thiam-api/docs/swagger.yaml -o ../thiam-api/docs/openapi.yaml",
    "api:generate": "openapi-typescript ../thiam-api/docs/openapi.yaml -o ./src/lib/api/generated/schema.ts",
    "api:update": "npm run api:convert && npm run api:generate",
    "api:watch": "npm run api:update -- --watch",
    "cypress:open": "cypress open",
    "cypress:open:mock": "cypress open --env MOCK_API=true",
    "cypress:run": "cypress run",
    "test:e2e": "cypress run --spec 'cypress/e2e/auth.cy.ts'",
    "test:e2e:mock": "cypress run --spec 'cypress/e2e/auth.cy.ts' --env MOCK_API=true",
    "test:e2e:headed": "cypress run --spec 'cypress/e2e/auth.cy.ts' --headed --browser chrome"
